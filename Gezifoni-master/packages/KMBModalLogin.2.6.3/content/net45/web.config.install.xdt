<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings xdt:Transform="InsertIfMissing">
	<add key="_MailHost" value="mail.domain.com"  xdt:Locator="Match(key)" xdt:Transform="InsertIfMissing" />
	<add key="_MailPort" value="587"  xdt:Locator="Match(key)" xdt:Transform="InsertIfMissing" />
    <add key="_MailUid" value="username@domail.com"  xdt:Locator="Match(key)" xdt:Transform="InsertIfMissing" />
    <add key="_MailPass" value="123456"  xdt:Locator="Match(key)" xdt:Transform="InsertIfMissing" />
  </appSettings>
  <connectionStrings xdt:Transform="InsertIfMissing">
    <add name="SampleDatabaseContext" connectionString="server={{ServerName}};database=KmbModalLoginSampleDb;user id={{UserId}};password={{Password}};" providerName="System.Data.SqlClient" xdt:Locator="Match(name)" xdt:Transform="InsertIfMissing" />
  </connectionStrings>
</configuration>